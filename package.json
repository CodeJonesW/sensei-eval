{
  "name": "sensei-eval",
  "version": "0.2.1",
  "description": "Evaluate AI-generated educational content with deterministic checks and LLM-as-judge scoring",
  "type": "module",
  "license": "MIT",
  "author": "Will Jones",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeJonesW/sensei-eval.git"
  },
  "homepage": "https://github.com/CodeJonesW/sensei-eval#readme",
  "bugs": {
    "url": "https://github.com/CodeJonesW/sensei-eval/issues"
  },
  "keywords": [
    "eval",
    "evaluation",
    "educational-content",
    "llm",
    "llm-judge",
    "ai",
    "content-quality",
    "rubric",
    "scoring"
  ],
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sensei-eval": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config tests/integration/vitest.config.ts",
    "analyze": "npx tsx scripts/analyze-discrimination.ts"
  },
  "files": [
    "dist",
    "action.yml"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
